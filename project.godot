; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[Pixel-Space-Background-Generator]

config/ninetailsrabbit.pixel_space_background_generator/update_notification_enabled=true

[application]

config/name="Pixel-Space-Background-Generator"
config/description="Port to Godot 4 for the pixel space background generator by deep-fold"
config/version="1.0.1"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/ninetailsrabbit.pixel_space_background_generator/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
